{
  "name": "timetracker",
  "description": "timetracker",
  "version": "0.0.4",
  "author": "Author",
  "main": "src/index.js",
  "private": true,
  "engines": {
    "node": ">=14",
    "yarn": "*"
  },
  "scripts": {
    "postinstall": "node package_symlinks.js",
    "start": "yarn postinstall && yarn start:api",
    "start:api": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --ignore 'src/e2e/**/*.ts' --ignore 'src/migrations/**/*.ts' --exec ts-node --files src/worker.ts",
    "build": "rimraf dist && tsc && cross-env SRCDIR=dist yarn postinstall",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "typecheck": "yarn tsc --noEmit",
    "test": "jest",
    "test:e2e:setup": "yarn build && dotenv -c test -- yarn migrate:reset",
    "test:e2e": "dotenv -c test -- jest --config=jest.e2e.config.ts --runInBand --testTimeout=10000",
    "test:e2e:detect-open-handles": "dotenv -c test -- jest --config=jest.e2e.config.ts --detectOpenHandles --runInBand --testTimeout=10000",
    "test:e2e:detect-leaks": "dotenv -c test -- jest --config=jest.e2e.config.ts --detectLeaks --runInBand --testTimeout=10000",
    "test:e2e:log-heap-usage": "dotenv -c test -- node --expose-gc ./node_modules/.bin/jest --config=jest.e2e.config.ts --runInBand --logHeapUsage --testTimeout=10000",
    "test:e2e:debug": "dotenv -c test -- node --inspect-brk --expose-gc ./node_modules/.bin/jest --config=jest.e2e.config.ts --runInBand --logHeapUsage --testTimeout=10000",
    "migrate": "yarn sequelize-cli db:migrate",
    "migrate:undo": "yarn sequelize-cli db:migrate:undo",
    "migrate:undo:all": "yarn sequelize-cli db:migrate:undo:all",
    "migrate:reset": "yarn migrate:undo:all && yarn migrate",
    "migrate:generate": "scripts/dev/generate-migration.sh",
    "seed": "dotenv -c test -- yarn sequelize-cli db:seed --seed",
    "seed:undo": "dotenv -c test -- yarn sequelize-cli db:seed:undo",
    "seed:undo:all": "dotenv -c test -- yarn sequelize-cli db:seed:undo:all",
    "seed:reset": "yarn seed:undo:all",
    "seed:generate": "scripts/dev/generate-seed.sh",
    "validate": "yarn lint && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-joi-openapi": "^0.7.0",
    "handlebars": "^4.7.7",
    "he": "^1.2.0",
    "helmet": "^3.23.3",
    "http-status": "^1.5.0",
    "ioredis": "^4.23.0",
    "joi": "^17.4.0",
    "jwt-simple": "^0.5.6",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.7",
    "openapi3-ts": "^2.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "6.16.3",
    "sequelize-typescript": "~2.1.3",
    "socket.io": "^3.1.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/fs-extra": "^9.0.6",
    "@types/he": "^1.1.1",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^27.0.1",
    "@types/joi": "^17.2.3",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.18.22",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.0",
    "@types/supertest": "~2.0.12",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "fs-extra": "^9.1.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "socket.io-client": "^3.1.3",
    "supertest": "~6.1.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^8.10.2",
    "typescript": "~4.1.6",
    "weak-napi": "^2.0.2"
  },
  "resolutions": {
    "deep-extend": "^0.5.1"
  }
}
